        },
        {
            "language": {
                "name": "en",
                "url": "http://localhost/api/v2/language/9/"
            },
            "text": "It holds Pok\u00e9athlon records. It is a\ncard that shows how many Link\nPok\u00e9athlon losses you have had.",
            "version_group": {
                "name": "x-y",
                "url": "http://localhost/api/v2/version-group/15/"
