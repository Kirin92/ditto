    "flavor_text_entries": [
        {
            "flavor_text": "\uff14\u672c\u306e\u3000\u30c4\u30ce\u306f\u3000\u9ad8\u6027\u80fd\u306e\u3000\u30ec\u30fc\u30c0\u30fc\u3002\n\u8033\u3084\u3000\u9f3b\u306e\u3000\u304b\u308f\u308a\u306b\n\u97f3\u3084\u3000\u306b\u304a\u3044\u3092\u3000\u611f\u3058\u53d6\u308b\u3002",
