        {
            "flavor_text": "\u3072\u3068\u306e\u3000\u3053\u3068\u3070\u3092\u3000\u308a\u304b\u3044\u3059\u308b\u3000\u305f\u304b\u3044\n\u3061\u306e\u3046\u3092\u3000\u3082\u3064\u3002\u3046\u307f\u306e\u3000\u3046\u3048\u3092\n\u3072\u3068\u3092\u3000\u306e\u305b\u3066\u3000\u3059\u3059\u3080\u306e\u304c\u3000\u3059\u304d\u3002",
