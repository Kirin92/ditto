    "flavor_text_entries": [
        {
            "flavor_text": "\u304a\u8179\u306e\u3000\u708e\u888b\u306e\u3000\u706b\u529b\u304c\n\u5f37\u307e\u308b\u307b\u3069\u3000\u901f\u304f\u3000\u98db\u3079\u308b\u304c\n\u70b9\u706b\u3059\u308b\u307e\u3067\u3000\u6642\u9593\u304c\u3000\u304b\u304b\u308b\u3002",
